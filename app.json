{
  "expo": {
    "name": "AIT_CrossPlatform_TaskManager",
    "slug": "AIT_CrossPlatform_TaskManager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/scribbles.png",
    "splash": {
      "image": "./assets/scribbles.png",
      "resizeMode": "contain",
      "backgroundColor": "#363636"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3b3b3b"
      }
    },
    "web": {
      "favicon": "./assets/VenmFace.png"
    }
  }
}
